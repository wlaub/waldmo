    <!-- top -->
    <set mask="x0x-111-111" tiles="8,2; 9,2; 10,2; 11,2"/>
    <!-- bottom -->
    <set mask="111-111-x0x" tiles="8,7; 9,7; 10,7; 11,7"/>
    <!-- left -->
