    <!-- corner top left -->
    <set mask="x0x-011-x11" tiles="7,2"/>
    <!-- corner top right -->
    <set mask="x0x-110-11x" tiles="12,2"/>
    <!-- corner bottom left -->
    <set mask="x11-011-x0x" tiles="7,7"/>
    <!-- corner bottom right -->
    <set mask="11x-110-x0x" tiles="12,7"/>
    
    <!-- 111 -->
    <!-- 111 -->   
    <!-- 110 -->      
    <!-- inside corner top left -->
    <set mask="111-111-110" tiles="1,3"/>
    <!-- inside corner bottom left -->
    <set mask="110-111-111" tiles="1,7"/>
    <!-- inside corner top right -->
    <set mask="111-111-011" tiles="5,3"/>
    <!-- inside corner bottom right -->
    <set mask="011-111-111" tiles="5,7"/>


    <!-- inner corners -->

    <!--   : -->
    <set mask="110-111-110" tiles="1,5"/>
    <!-- `` -->
    <set mask="010-111-111" tiles="3,7"/>
    <!-- :  -->
    <set mask="011-111-011" tiles="5,5"/>
    <!-- .. -->
    <set mask="111-111-010" tiles="3,3"/>

    <!-- `: -->
    <set mask="010-111-110" tiles="7,11"/>
    <!-- :` -->
    <set mask="010-111-011" tiles="5,15"/>
    <!-- :. -->
    <set mask="011-111-010" tiles="5,13"/>
    <!-- .: -->
    <set mask="110-111-010" tiles="3,13"/>
    <!-- :: -->
    <set mask="010-111-010" tiles="3,15"/>
    <!-- .` -->
    <set mask="110-111-011" tiles="7,13"/>
    <!-- `. -->
    <set mask="011-111-110" tiles="7,15"/>

    <!-- inner corner variants -->
    <!-- corner top left -->
    <!-- x0x -->
    <!-- 011 -->
    <!-- x10 -->
    <set mask="x0x-011-x10" tiles="1,9"/>
    <!-- corner top right -->
    <set mask="x0x-110-01x" tiles="9,9"/>
    <!-- corner bottom left -->
    <set mask="x10-011-x0x" tiles="1,17"/>
    <!-- corner bottom right -->
    <set mask="01x-110-x0x" tiles="9,17"/>

    <!--  |: -->
    <set mask="x10-011-x10" tiles="1,15"/>
    <!-- _``_ -->
    <set mask="010-111-x0x" tiles="3,17"/>
    <!-- :| -->
    <set mask="01x-110-01x" tiles="9,11"/>
    <!-- ~..~ -->
    <set mask="x0x-111-010" tiles="7,9"/>

    <!--  |. -->
    <set mask="x11-011-x10" tiles="1,13"/>
    <!--  |` -->
    <set mask="x10-011-x11" tiles="1,11"/>
 
    <!-- __`_-->
    <set mask="110-111-x0x" tiles="7,17"/>
    <!-- _`__ -->
    <set mask="011-111-x0x" tiles="5,17"/>

    <!-- .| -->
    <set mask="11x-110-01x" tiles="9,15"/>
    <!-- `| -->
    <set mask="01x-110-11x" tiles="9,13"/>

    <!-- ~~.~ -->
    <set mask="x0x-111-110" tiles="5,9"/>
    <!-- ~.~~ -->
